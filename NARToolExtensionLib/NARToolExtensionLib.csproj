<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <RootNamespace>Nexon.Extension</RootNamespace>
    <ApplicationVersion>2.2.0.0</ApplicationVersion>
    <BaseOutputPath>..\bin</BaseOutputPath>
    <Title>Nexon Archive Tools Extension Library</Title>
    <Product>NARTools</Product>
    <Copyright>Copyright © Smilex_Gamer 2024</Copyright>
    <Version>2.2.0.0</Version>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="ICSharpCode.SharpZipLib">
      <HintPath>..\ICSharpCode.SharpZipLib.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\NARToolLib\NARToolLib.csproj" />
  </ItemGroup>

</Project>
